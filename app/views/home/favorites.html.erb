  <h1>Most Popular dogs</h1>
  <div class="row">
    <div class="large-12 columns">
      <ul class="small-block-grid-6">
        <ul class="small-block-grid-6">
          <% Dog.sort_by_likes.first(6).each do |dog| %>
          <li>
            <%= link_to image_tag(dog.picture.thumb.url, alt: "image"), dog_path(dog) %>
            <br>
            <h4><%= dog.name %></h4>
          </li>
          <% end %>
        </ul>
      </ul>
    </div>
  </div>
</div>
</li>
